{
    "name": "mahakaal/manu-latex-bundle",
    "minimum-stability": "RC",
    "description": "Latex and pdf generator for Symfony 2",
    "keywords": ["latex", "pdf", "generator", "symfony"],
    "type": "symfony-bundle",
    "homepage": "http://github.com/mahakaal/latex-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob van de Vijver",
            "email": "bobvandevijver@hotmail.com"
        }
    ],
    "require": {
        "symfony/config": ">= 2.3.0",
        "symfony/console": ">= 2.3.0",
        "symfony/dependency-injection": ">= 2.3.0",
        "symfony/filesystem": ">= 2.3.0",
        "symfony/finder": ">= 2.3.0",
        "symfony/form": ">= 2.3.0",
        "symfony/framework-bundle": ">= 2.3.0",
        "symfony/http-foundation": ">= 2.3.0",
        "symfony/http-kernel": ">= 2.3.0",
        "symfony/options-resolver": ">= 2.3.0",
        "symfony/process": ">= 2.3.0",
        "twig/twig": ">= 1.9.0"
    },
    "suggest": {
        "egeloen/ckeditor-bundle": "Allows the usage of the bobv_latex form type, and easily integrates ckeditor into your Symfony application",
        "spatie/pdf-to-image": "Allows you to convert PDF output into a image."
    },
    "autoload": {
        "psr-4": {
            "BobV\\LatexBundle\\": ""
        }
    }
}
